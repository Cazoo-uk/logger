  "license": "ISC",
  "devDependencies": {
    "@types/node": "^12.0.8",
    "@types/pino": "^5.8.8",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "aws-lambda": "^0.1.2",
    "cross-env": "^5.2.0",
    "flush-write-stream": "^2.0.0",
    "qodaa": "^1.0.1",
    "snazzy": "^8.0.0",
    "split2": "^3.1.1",
    "standard": "^12.0.1",
    "tap": "^14.2.3",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "eslint-plugin-typescript": "^0.14.0",
    "pino": "^5.12.6"
  },
    "publishConfig": {
